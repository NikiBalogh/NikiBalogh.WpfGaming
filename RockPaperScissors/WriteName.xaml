<Window x:Class="RockPaperScissors.WriteName"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RockPaperScissors"
        mc:Ignorable="d"
        Title="WriteName" Height="200" Width="500"
        ResizeMode="NoResize">
    <Grid>
        <Label Content="Skriv dit navn" HorizontalAlignment="Left" Margin="22,27,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textboxUserinput" HorizontalAlignment="Left" Margin="22,130,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="451"/>
        <Button x:Name="btnConfirm" Content="OK" HorizontalAlignment="Left" Margin="408,30,0,0" VerticalAlignment="Top" Width="65" Height="23" Click="btnConfirm_Click"/>

    </Grid>
</Window>
